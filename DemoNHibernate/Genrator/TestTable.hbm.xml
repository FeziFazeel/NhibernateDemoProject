<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="DemoNHibernate" namespace="DemoNHibernate" xmlns="urn:nhibernate-mapping-2.2">
  <class name="TestTable" table="test_table" lazy="true" >
    <id name="Id" column="id" />
    <property name="Name">
      <column name="name" sql-type="varchar(45)" not-null="false" />
    </property>
    <property name="Age">
      <column name="age" sql-type="int(11)" not-null="false" />
    </property>
    <property name="City">
      <column name="city" sql-type="varchar(45)" not-null="false" />
    </property>
  </class>
</hibernate-mapping>